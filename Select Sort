#include <iostream>
#include <algorithm>
#include <vector>

using namespace std;

//rewrite selectionSort function with vector instead of array
vector<int> selectionSort2(vector<int> arr){
	int temp;
	for(int i = 0; i < arr.size(); i++){
		int minIndex = i;
		for(int j = i + 1; j < arr.size(); j++){
			if(arr[j] < arr[minIndex]){
				minIndex = j;
			}
			
		}
		temp = arr[i];
		arr[i] = arr[minIndex];
		arr[minIndex] = temp;
	}
	return arr;
}

int main() {
	vector<int> a;
	vector<int> b;
	for(int i = 0; i < 100; i++){
		a.push_back(100 - i);
	}
    b = selectionSort2(a);
    for(unsigned x = 0; x < b.size(); x++){
    	cout << b[x] << ", ";
	}
    cout<<endl;
	a.clear();
	b.clear();
    return 0;
}
